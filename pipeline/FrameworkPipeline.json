							"linkedServiceName": {
								"referenceName": "AzureSqlDatabaseDestination",
								"type": "LinkedServiceReference",
								"parameters": {
									"databasename": "dev_renaliq_dm_all"
								}
							}
						}
					]
				}
			}
		],
		"parameters": {
